<%= form_with(model: jobprofile , local: true ) do |form| %>
    <% if jobprofile.errors.any? %>
      <div id="error_explanation">
        <h2><%= pluralize(jobprofile.errors.count, "error") %> prohibited this user from being saved:</h2>
        
  
        <ul>
          <% jobprofile.errors.full_messages.each do |message| %>
            <li><%= message %></li>
          <% end %>
        </ul>
      </div>
    <% end %>
    <div class="field">
      <%= form.label :adresse %>
      <%= form.text_field :adresse %>
    </div>
    <div class="field">
      <%= form.label :ville %>
      <%= form.text_field :ville %>
    </div>
    <div class="field">
      <%= form.label :gallery %>
      <%= form.file_field :gallery, multiple: true %>
    </div>
    <div class="field">
      <%= form.label :tarif %>
      <%= form.text_field :tarif %>
    </div>
    <div class="field">
  <%= form.label :namejob %><br />
 
  <%= form.select :namejob, Job.all.collect { |p| [ p.name ] }, include_blank: true %>
</div>
    
  
    <div class="actions">
      <%= form.submit %>
    </div>
  <% end %>
  